{
    "markdownlint.config": {
        "no-inline-html": false
    },
    // Compile sources
    "java.project.sourcePaths": [
        "2024Java",
        "2023Java"
    ],
    // Default classpath search place
    // Clean plugin cache on each modification.
    "java.project.outputPath": "compileRuntime/bin",
    "java.project.referencedLibraries": [],
    "files.associations": {
        "*.cpp": "cpp",
        "*.c": "c",
        ".py": "python",
        "*.stact": "st",
        "*.stdut": "st",
        "*.stfb": "st",
        "*.vim": "Vimscript",
        "_vimrc": "Vimscript",
        "string": "cpp",
        "ostream": "cpp",
        "vector": "cpp",
        "*.tcc": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "print": "cpp",
        "format": "cpp",
        "array": "cpp",
        "bitset": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "ranges": "cpp",
        "span": "cpp",
        "stacktrace": "cpp",
        "regex": "cpp",
        "valarray": "cpp",
        "sstream": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "generator": "cpp",
        "text_encoding": "cpp"
    },
