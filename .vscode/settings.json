{
    "markdownlint.config": {
        "no-inline-html": false
    },
    // Compile sources
    "java.project.sourcePaths": [
        "2024Java",
        "2023Java"
    ],
    // Default classpath search place
    // Clean plugin cache on each modification.
    "java.project.outputPath": "compileRuntime/bin",
    "java.project.referencedLibraries": [],
    "files.associations": {
        "*.cpp": "cpp",
        "*.c": "c",
        ".py": "python",
        "*.stact": "st",
        "*.stdut": "st",
        "*.stfb": "st",
        "*.vim": "Vimscript",
        "_vimrc": "Vimscript",
        "string": "cpp",
        "ostream": "cpp",
        "vector": "cpp",
        "*.tcc": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp"
    },
    // "java.project.referencedLibraries": [
    //     "2024Java/lib/**/*.jar"
    // ],
}